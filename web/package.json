{
  "name": "conquest-eth-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.1",
    "@ethersproject/solidity": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/wallet": "^5.5.0",
    "@ethersproject/web": "^5.6.0",
    "@sentry/browser": "^6.16.1",
    "@sentry/integrations": "^6.16.1",
    "@sentry/tracing": "^6.16.1",
    "@urql/core": "2.3.6",
    "@urql/devtools": "^2.0.3",
    "aes-js": "https://github.com/wighawag/aes-js#ddcec7d95eb5608c4fe023f9f035b7e469b50809",
    "byte-base64": "^1.1.0",
    "flatpickr": "^4.6.9",
    "graphql": "^16.2.0",
    "idb": "^7.0.0",
    "lz-string": "^1.4.4",
    "named-logs": "^0.1.0",
    "pretty-ms": "https://github.com/wighawag/pretty-ms#320ba46c464052ebbe605e48ad05df9e9801e956",
    "sveltejs-tippy": "^3.0.0",
    "web3w": "^0.3.2-beta.23",
    "web3w-portis-loader": "^0.3.3",
    "web3w-walletconnect-loader": "^0.3.2",
    "wonka": "^4.0.15"
  },
  "devDependencies": {
    "@sentry/cli": "^1.72.0",
    "@sveltejs/adapter-node": "1.0.0-next.58",
    "@sveltejs/adapter-static": "1.0.0-next.24",
    "@sveltejs/kit": "1.0.0-next.212",
    "@types/aes-js": "^3.1.1",
    "@types/lz-string": "^1.3.34",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "autoprefixer": "^10.4.1",
    "bech32": "^2.0.0",
    "bn.js": "^5.2.0",
    "conquest-eth-common": "workspace:*",
    "cssnano": "^5.0.14",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "hash.js": "^1.1.7",
    "ipfs-deploy": "^11.1.0",
    "ipfs-gateway-emulator": "4.2.1-ipfs.2",
    "js-sha3": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prepare-for-web": "^0.1.7",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.44.3",
    "svelte-preprocess": "^4.10.1",
    "sveltejs-adapter-ipfs": "0.2.1",
    "tailwindcss": "^3.0.11",
    "tailwindcss-question-mark": "^0.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "2.7.10"
  },
  "scripts": {
    "prepare": "node ./.setup.cjs && prepare-for-web --config src/application.json --target static --targetFile src/app.html",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --plugin-search-dir=. --check .",
    "format:fix": "prettier --plugin-search-dir=. --write .",
    "dev": "svelte-kit dev --host 0.0.0.0",
    "dev:inspect": "npm --node-options --inspect run dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "serve": "ipfs-emulator --only -d build -p 8080",
    "sentry": "pnpm sentry-cli releases --org etherplay new -p conquest-eth `git rev-parse --short HEAD` && pnpm sentry-cli releases --org etherplay -p conquest-eth files `git rev-parse --short HEAD` upload-sourcemaps release/build && pnpm sentry-cli releases --org etherplay -p conquest-eth set-commits --ignore-missing --ignore-empty --auto `git rev-parse --short HEAD` && pnpm sentry-cli releases --org etherplay -p conquest-eth finalize `git rev-parse --short HEAD`",
    "deploy": "dotenv -- ipfs-deploy -p pinata -s conquest-eth build",
    "build:deploy": "run-s build deploy"
  },
  "volta": {
    "node": "14.17.5"
  }
}
