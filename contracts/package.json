{
  "name": "conquest-eth-contracts",
  "version": "0.1.1",
  "description": "conquest-eth ethereum smart contracts",
  "private": true,
  "files": [
    "export/artifacts/",
    "export/deployments/",
    "export/deploy_l2/",
    "export/utils/",
    "src/outerspace/interfaces/",
    "src/outerspace/events/ImportingOuterSpaceEvents.sol",
    "src/outerspace/types/ImportingOuterSpaceTypes.sol"
  ],
  "engines": {
    "node": ">= 14.0.0"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.3",
    "@ethersproject/solidity": "^5.5.0",
    "@ethersproject/transactions": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/wallet": "^5.5.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@0.3.0-beta.13",
    "@openzeppelin/contracts": "^4.3.2",
    "@typechain/ethers-v5": "^9.0.0",
    "@typechain/hardhat": "^4.0.0",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/progress": "^2.0.5",
    "@types/qrcode": "1.4.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@urql/core": "^2.4.3",
    "chai": "^4.3.6",
    "chai-ethers": "^0.0.1",
    "conquest-eth-common": "workspace:*",
    "cross-env": "^7.0.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "ethereum-transfer-gateway": "^0.1.0-beta.9",
    "ethers": "^5.5.4",
    "fs-extra": "^10.0.1",
    "hardhat": "^2.9.0",
    "hardhat-contract-sizer": "^2.5.0",
    "hardhat-deploy": "^0.11.10",
    "hardhat-deploy-tenderly": "^0.1.1",
    "hardhat-gas-reporter": "^1.0.8",
    "isomorphic-unfetch": "^3.1.0",
    "mocha": "^9.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "progress": "^2.0.3",
    "qrcode": "^1.5.0",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.20",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.6.0",
    "typechain": "^7.0.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "prepare": "node ./.setup.js && hardhat typechain",
    "lint": "eslint \"**/*.{js,ts}\" && solhint src/**/*.sol",
    "lint:fix": "eslint --fix \"**/*.{js,ts}\" && solhint --fix src/**/*.sol",
    "format": "prettier --plugin-search-dir=. --check \"**/*.{ts,js,sol}\"",
    "format:fix": "prettier --plugin-search-dir=. --write \"**/*.{ts,js,sol}\"",
    "compile": "hardhat compile",
    "export:artifacts": "tsc -p tsconfig.deploy.json && hardhat export-artifacts export/artifacts --hide-sources --sources-for BasicAlliance,AllianceRegistry",
    "void:deploy": "hardhat deploy --report-gas",
    "test": "cross-env HARDHAT_DEPLOY_FIXTURE=true HARDHAT_COMPILE=true mocha --bail --recursive test",
    "single:test": "cross-env HARDHAT_COMPILE=true mocha --bail --recursive test",
    "gas": "cross-env REPORT_GAS=true hardhat test",
    "coverage": "cross-env HARDHAT_DEPLOY_FIXTURE=true hardhat coverage",
    "dev:node": "cross-env MINING_INTERVAL=\"3000,5000\" hardhat node --hostname 0.0.0.0",
    "dev": "cross-env MINING_INTERVAL=\"3000,5000\" hardhat node --hostname 0.0.0.0 --watch",
    "local:dev": "hardhat --network localhost deploy --watch",
    "execute": "node ./_scripts.js run",
    "deploy": "node ./_scripts.js deploy",
    "verify": "node ./_scripts.js verify",
    "export": "node ./_scripts.js export",
    "metadata": "node ./_scripts.js metadata",
    "hardhat": "hardhat",
    "fork:execute": "node ./_scripts.js fork:run",
    "fork:deploy": "node ./_scripts.js fork:deploy",
    "fork:dev": "node ./_scripts.js fork:dev",
    "fork:node": "node ./_scripts.js fork:node",
    "fork:test": "node ./_scripts.js fork:test",
    "tenderly:push": "node ./_scripts.js tenderly:push"
  }
}
